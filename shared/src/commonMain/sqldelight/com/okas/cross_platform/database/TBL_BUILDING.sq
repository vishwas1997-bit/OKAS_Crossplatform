CREATE TABLE TBL_BUILDING (
    building_id TEXT NOT NULL PRIMARY KEY,
    building_name TEXT NOT NULL,
    num_floors TEXT NOT NULL,
    building_image TEXT NOT NULL,
    num_users TEXT NOT NULL,
    building_type TEXT NOT NULL
);

insertBuildingInfo:
INSERT OR REPLACE INTO TBL_BUILDING (
  building_id,
  building_name,
  num_floors,
  building_image,
  num_users,
  building_type
)
VALUES (
  ?,
  ?,
  ?,
  ?,
  ?,
  ?
);